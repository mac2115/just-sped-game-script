local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Midnight sun v2.70 by Mac#0014", "DarkTheme")

local Tab = Window:NewTab("Autofarm")

local Section = Tab:NewSection("Autofarms(use noclip to avoid glitches)")

Section:NewToggle(
    "Autofarm",
    "Toggles Autofarm",
    function(state)
        if state then
            getgenv().autofarm = true
        else
            getgenv().autofarm = false
        end
        
        killauraforautofarm()
        autofarm(demon)
    end
)

Section:NewDropdown(
    "Choose Mob",
    "Chooses mob for the autofarm to farm",
    {"Weak", "Strong", "Ultra", "Mizunoto", "Kanato", "Tsuchinoe", "Hinoe"},
    function(currentOption)
        demon = currentOption
    end
)
Section:NewToggle(
    "Auto grip(for slayer npcs)",
    "Automaticaly grips slayer npcs",
    function(state)
        if state then
            getgenv().autogrip = true
        else
            getgenv().autogrip = false
        end
        autogrip()
    end
)

Section:NewSlider(
    "Farm position",
    "Farm position",
    7,
    -6,
    function(s) -- 500 (MaxValue) | 0 (MinValue)
        positionz = s
    end
)

local Section = Tab:NewSection("Autofarm Bosses(use  godmode)")

