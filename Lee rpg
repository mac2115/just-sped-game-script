local function findnpc()
    spawn(function()
        if getgenv().findnpc == true then

            while getgenv().findnpc and wait() do
                if npclookingfor == "Merchant" then 
for i, v in pairs(game:GetService("Workspace"):GetChildren()) do
    if string.find(v.Name, npclookingfor) and v.Name ~= OldManYochiro() and not v:FindFirstChild("Shadow A. Mask")  then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
    
    end
end
elseif npclookingfor == "Injured" then 

    for i, v in pairs(game:GetService("Workspace"):GetChildren()) do
        if string.find(v.Name, npclookingfor) and v.Name ~= OldManYochiro() and not v:FindFirstChild("Shadow A. Mask") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
        
        end
    end

elseif npclookingfor == "OldMan" then 
    for i, v in pairs(game:GetService("Workspace"):GetChildren()) do
        if string.find(v.Name, npclookingfor) and v.Name ~= OldManYochiro() and not v:FindFirstChild("Shadow A. Mask")  then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
        
        end
    end
elseif npclookingfor == "Muzan" then 
    for i, v in pairs(game:GetService("Workspace"):GetChildren()) do
        if string.find(v.Name, npclookingfor) and v.Name ~= OldManYochiro() and not v:FindFirstChild("Shadow A. Mask")  then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
        
        end
    end

else
    return

end

end
elseif npclookingfor == "Muzan" then
for i, v in pairs(game:GetService("Workspace"):GetChildren()) do
    if string.find(v.Name, npclookingfor) and v.Name ~= OldManYochiro() and not v:FindFirstChild("Shadow A. Mask") then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
    else
        return
end
end
else
    return
end
end)
end





local function noclip()
    game:GetService("RunService").Stepped:Connect(
        function()
            if getgenv().noclip then
                pcall(
                    function()
                        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            if v.ClassName == "Part" or v.ClassName == "MeshPart" then
                                v.CanCollide = false
                            end
                        end
                    end
                )
            else
                pcall(
                    function()
                        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                            if v.ClassName == "Part" or v.ClassName == "MeshPart" then
                                v.CanCollide = true
                            end
                        end
                    end
                )
            end
        end
    )
end

