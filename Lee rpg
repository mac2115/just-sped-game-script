local function players()
spawn(function()
if getgenv().playerfarm == true then
    if game:GetService("Workspace").Live.Players:FindFirstChild(player)  then
while game:GetService("Workspace").Live.Players:FindFirstChild(player) and getgenv().playerfarm   do task.wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Live.Players[player].HumanoidRootPart.CFrame * CFrame.new(0, 0, positionz1)
    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteEvent:FireServer("M2")
    game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Tool").RemoteEvent:FireServer("Hitbox",1,"M2")
end
else
    return
    end
else
   return

 end



end)
end


